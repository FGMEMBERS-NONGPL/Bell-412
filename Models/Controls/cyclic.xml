<?xml version="1.0"?>
<PropertyList> 
	<path>cyclic.ac</path>
	<animation>
		<type>translate</type>
		<object-name>5.Cockpit Control Cyclic Pilot Switch ForceTrim</object-name>
		<property>controls/flight/cyclic/force-trim-release</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>0.006</dep>
			</entry>
		</interpolation>
		<axis>
			<x>0</x>
			<y>0.6</y>
			<z>-1</z>
		</axis>
	</animation>
	<animation> 
		<type>rotate</type>
		<object-name>5.Cockpit Control Cyclic Pilot</object-name>
		<!-- <property>controls/flight/elevator</property> -->
		<property>controls/flight/servos/pitch</property>
		<interpolation> 
			<entry> 
				<ind>-1.0</ind>
				<dep>-13</dep>
			</entry>
			<entry> 
				<ind>0.0</ind>
				<dep>0</dep>
			</entry>
			<entry> 
				<ind>1.0</ind>
				<dep>13</dep>
			</entry>
		</interpolation>
		<axis> 
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
        <center>
            <x-m>-2.69997</x-m>
            <y-m>0.56718</y-m>
            <z-m>-0.16486</z-m>
        </center>
	</animation>
	<animation> 
		<type>rotate</type>
		<object-name>5.Cockpit Control Cyclic Pilot</object-name>
		<!-- <property>controls/flight/aileron</property>-->
		<property>controls/flight/servos/roll</property>
		<interpolation> 
			<entry> 
				<ind>-1.0</ind>
				<dep>-15</dep>
			</entry>
			<entry> 
				<ind>0.0</ind>
				<dep>0</dep>
			</entry>
			<entry> 
				<ind>1.0</ind>
				<dep>15</dep>
			</entry>
		</interpolation>
		<axis> 
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
        <center>
            <x-m>-2.69997</x-m>
            <y-m>0.56718</y-m>
            <z-m>-0.16486</z-m>
        </center>
	</animation>
</PropertyList> 
