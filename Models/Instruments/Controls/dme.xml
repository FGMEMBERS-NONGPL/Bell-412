<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
	<path>dme.ac</path>

	<!-- 
		 General settings
	-->
	<animation>
		<object-name>5.Cockpit Panel Main Control DME Knob</object-name>
		<object-name>DME_NM_10</object-name>
		<object-name>DME_NM_1</object-name>
		<object-name>DME_NM_.1</object-name>
		<object-name>DME_NM_dot</object-name>
		<object-name>DME_NM_id</object-name>
		<object-name>DME_KT_100</object-name>
		<object-name>DME_KT_10</object-name>
		<object-name>DME_KT_1</object-name>
		<object-name>DME_KT_id</object-name>
		<object-name>DME_MIN_10</object-name>
		<object-name>DME_MIN_1</object-name>
		<object-name>DME_MIN_id</object-name>
	</animation>

	<animation>
		<type>noshadow</type>
		<object-name>DME_NM_10</object-name>
		<object-name>DME_NM_1</object-name>
		<object-name>DME_NM_.1</object-name>
		<object-name>DME_NM_dot</object-name>
		<object-name>DME_NM_id</object-name>
		<object-name>DME_KT_100</object-name>
		<object-name>DME_KT_10</object-name>
		<object-name>DME_KT_1</object-name>
		<object-name>DME_KT_id</object-name>
		<object-name>DME_MIN_10</object-name>
		<object-name>DME_MIN_1</object-name>
		<object-name>DME_MIN_id</object-name>
	</animation>
	
	<effect>
    	<condition>
      		<property>sim/rendering/rembrandt/enabled</property>
    	</condition>
    	<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>DME_NM_10</object-name>
		<object-name>DME_NM_1</object-name>
		<object-name>DME_NM_.1</object-name>
		<object-name>DME_NM_dot</object-name>
		<object-name>DME_NM_id</object-name>
		<object-name>DME_KT_100</object-name>
		<object-name>DME_KT_10</object-name>
		<object-name>DME_KT_1</object-name>
		<object-name>DME_KT_id</object-name>
		<object-name>DME_MIN_10</object-name>
		<object-name>DME_MIN_1</object-name>
		<object-name>DME_MIN_id</object-name>
	</effect>

	<!-- 
		 Selection
	-->
	<animation>
		<type>select</type>
		<object-name>DME_Nm</object-name>
		<object-name>DME_Kt</object-name>
		<object-name>DME_Min</object-name>
		<condition>
			<and>
				<property>instrumentation/dme/powered</property>
				<property>instrumentation/dme/switch-position</property>
			</and>
		</condition>
	</animation>

	<!-- 
		 Lighting
	-->
	<animation>
		<type>material</type>
		<object-name>DME_Nm</object-name>
		<object-name>DME_NM_10</object-name>
		<object-name>DME_NM_1</object-name>
		<object-name>DME_NM_.1</object-name>
		<object-name>DME_NM_dot</object-name>
		<object-name>DME_NM_id</object-name>
		<object-name>DME_Kt</object-name>
		<object-name>DME_KT_100</object-name>
		<object-name>DME_KT_10</object-name>
		<object-name>DME_KT_1</object-name>
		<object-name>DME_KT_id</object-name>
		<object-name>DME_Min</object-name>
		<object-name>DME_MIN_10</object-name>
		<object-name>DME_MIN_1</object-name>
		<object-name>DME_MIN_id</object-name>
		<condition>
			<property>instrumentation/dme/powered</property>
    	</condition>
		<ambient>
			<red>0.0</red>
			<green>0.0</green>
			<blue>0.0</blue>
		</ambient>
		<diffuse>
			<red>0.0</red>
			<green>0.0</green>
			<blue>0.0</blue>
		</diffuse>
		<emission>
			<red>0.8</red>
			<green>0.4</green>
			<blue>0.0</blue>
		</emission>
	</animation>

	<!-- 
		 Animation
	-->
    <animation>
        <type>knob</type>
		<object-name>5.Cockpit Panel Main Control DME Knob</object-name>
		<property>instrumentation/dme/switch-position</property>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0.00255</x-m>
            <y-m>0.00</y-m>
            <z-m>-0.00825</z-m>
		</center>
		<interpolation> 
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>		
			<entry>
				<ind>1</ind>
				<dep>25</dep>
			</entry>
			<entry>
				<ind>2</ind>
				<dep>160</dep>
			</entry>
		</interpolation>
        <action>
            <binding>
                <command>property-adjust</command>
				<property>instrumentation/dme/switch-position</property>
                <factor>1</factor>
                <min>0</min>
                <max>2</max>
                <wrap>false</wrap>
            </binding>
        </action>
	</animation>

	<!-- 
		 Texture Translation
	-->
    <animation>
        <type>textranslate</type>
		<object-name>DME_NM_10</object-name>
		<property>instrumentation/dme/indicated-distance-nm</property>
        <step>10</step>
        <factor>0.01</factor>
        <bias>0.0001</bias>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
	</animation>
    <animation>
        <type>textranslate</type>
		<object-name>DME_NM_1</object-name>
		<property>instrumentation/dme/indicated-distance-nm</property>
        <step>1</step>
        <factor>0.1</factor>
        <bias>0.0001</bias>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>textranslate</type>
		<object-name>DME_NM_.1</object-name>
		<property>instrumentation/dme/indicated-distance-nm</property>
        <step>0.1</step>
        <factor>1</factor>
        <bias>0.0001</bias>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>textranslate</type>
		<object-name>DME_KT_100</object-name>
		<property>instrumentation/dme/indicated-ground-speed-kt</property>
        <step>100</step>
        <factor>0.001</factor>
        <bias>0.0001</bias>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>textranslate</type>
		<object-name>DME_KT_10</object-name>
		<property>instrumentation/dme/indicated-ground-speed-kt</property>
        <step>10</step>
        <factor>0.01</factor>
        <bias>0.0001</bias>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>textranslate</type>
		<object-name>DME_KT_1</object-name>
		<property>instrumentation/dme/indicated-ground-speed-kt</property>
        <step>1</step>
        <factor>0.1</factor>
        <bias>0.0001</bias>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>textranslate</type>
		<object-name>DME_MIN_10</object-name>
		<property>instrumentation/dme/time-min</property>
        <step>10</step>
        <factor>0.01</factor>
        <bias>0.0001</bias>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>textranslate</type>
		<object-name>DME_MIN_1</object-name>
		<property>instrumentation/dme/time-min</property>
        <step>1</step>
        <factor>0.1</factor>
        <bias>0.0001</bias>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
</PropertyList>
