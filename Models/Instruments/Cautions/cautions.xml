						<property>bell412/power/buses/bus[1]/gentemp</property>
						<value>54</value>
					</greater-than>
					<property>bell412/power/buses/state</property>
				</and>
				<property>controls/cautions/switches/test</property>
			</or>
    	</condition>
	</animation>
	

	<!-- ENGINE OUT 1 (main board) -->
	<animation>
		<type>select</type>
		<object-name>5.Cockpit Panel Main Cautions.1.OUTSTATE</object-name>
		<condition>
			<or>
