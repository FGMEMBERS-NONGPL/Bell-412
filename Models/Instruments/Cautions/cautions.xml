					<not><property>controls/engines/engine[1]/hydr</property></not>
					<property>bell412/power/buses/state</property>
				</and>
				<property>controls/cautions/switches/test</property>
			</or>
    	</condition>
	</animation>
	<!-- E2 OILPRESS -->
	<animation>
		<type>select</type>
		<object-name>5.Cockpit Panel Main Cautions.2.OILPRESSURE</object-name>
		<condition>
			<or>
				<and>
					<less-than>
						<property>bell412/mechanics/engines/engine[1]/oilpress</property>
						<value>40</value>
					</less-than>
					<property>bell412/power/buses/state</property>
				</and>
				<property>controls/cautions/switches/test</property>
			</or>
    	</condition>
	</animation>

	<!-- RADIO ALT DH DESIRED HEIGHT -->
	<animation>
		<type>select</type>
		<object-name>5.Cockpit Panel Main Cautions.RALTDH</object-name>
		<condition>
			<or>
				<and>
					<property>controls/instruments/radioalt/alarm</property>
					<property>bell412/power/buses/state</property>
				</and>
				<property>controls/cautions/switches/test</property>
			</or>
		</condition>
	</animation>

	
