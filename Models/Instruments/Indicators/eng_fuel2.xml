<?xml version="1.0"?>

<PropertyList>
	<path>eng_fuel.ac</path>

	<animation>
		<object-name>5.Cockpit Panel Main Instruments FUELQTY Needle</object-name>
	</animation>

    <animation>
        <type>rotate</type>
		<object-name>5.Cockpit Panel Main Instruments FUELQTY Needle</object-name>
		<property>bell412/fuel/engine[1]/level-lbs</property>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
		</axis>
  		<interpolation>
            <entry><ind>-9999</ind><dep>-40</dep></entry>
            <entry><ind>0</ind><dep>-40</dep></entry>
            <entry><ind>500</ind><dep>-75</dep></entry>
            <entry><ind>1000</ind><dep>-105</dep></entry>
            <entry><ind>1500</ind><dep>-135</dep></entry>
            <entry><ind>2000</ind><dep>-165</dep></entry>
            <entry><ind>9999</ind><dep>-165</dep></entry>
        </interpolation>
    </animation>
</PropertyList>
