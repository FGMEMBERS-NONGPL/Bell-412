<?xml version="1.0"?>
<PropertyList>
	<path>RadioAlt_set.ac</path>
    <object-name>5.Cockpit Panel Main Instruments RadioAlt Needdle Set</object-name>
	<object-name>5.Cockpit Panel Main Instruments RadioAlt Needdle Off</object-name>
	<offsets>
		<x-m>-2.79256</x-m>
		<y-m>0.5555</y-m>
		<z-m>0.23317</z-m>
		<pitch-deg>-8.39</pitch-deg>
	</offsets>
    <animation>
        <type>rotate</type>
        <object-name>5.Cockpit Panel Main Instruments RadioAlt Needdle Set</object-name>
		<property>controls/instruments/radioalt/set</property>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
		</axis>
  		<interpolation>
            <entry><ind>-999</ind><dep>0</dep></entry>
            <entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>50</ind><dep>47</dep></entry>
            <entry><ind>100</ind><dep>90</dep></entry>
            <entry><ind>200</ind><dep>180</dep></entry>
            <entry><ind>300</ind><dep>188</dep></entry>
            <entry><ind>400</ind><dep>196</dep></entry>
            <entry><ind>500</ind><dep>204</dep></entry>
            <entry><ind>1000</ind><dep>248</dep></entry>
            <entry><ind>1500</ind><dep>290</dep></entry>
            <entry><ind>2000</ind><dep>300</dep></entry>
            <entry><ind>30000</ind><dep>300</dep></entry>
		</interpolation>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>5.Cockpit Panel Main Instruments RadioAlt Needdle Off</object-name>
		<condition>
			<equals>
				<property>controls/instruments/radioalt/set</property>
				<value>0</value>
			</equals>
    	</condition>
	</animation>
</PropertyList>
