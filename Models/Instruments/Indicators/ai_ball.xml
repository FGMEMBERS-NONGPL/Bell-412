		<axis>
			<x> 1 </x>
			<y> 0 </y>
			<z> 0 </z>
		</axis>
		<center>
			<x-m>  2.89490 </x-m>
			<y-m>  0.44069 </y-m>
			<z-m>  0.42101 </z-m>
		</center>
