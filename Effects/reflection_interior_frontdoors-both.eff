<?xml version="1.0" encoding="utf-8"?>

<PropertyList>
	<name>reflection_interior_frontdoors-both</name>
	<inherits-from>Effects/model-combined-transparent</inherits-from>
	<parameters>
		<reflection-enabled type="int">
			<use>controls/options/cockpit-texture-reflection</use>
		</reflection-enabled>
		<reflection-dynamic type="int">0</reflection-dynamic>
		<reflect-map-enabled type="int">1</reflect-map-enabled>
		<reflection-correction type="float">
			<use>controls/effects/reflection-correction</use>
		</reflection-correction>
    	<reflection-fresnel type="float">0.0</reflection-fresnel>
    	<reflection-rainbow type="float">0.4</reflection-rainbow>
		<reflection-noise type="float">0.0</reflection-noise>
		<rain-enabled type="int">2</rain-enabled>
		<texture n="5">
			<type>cubemap</type>
			<images>
				<positive-x>Aircraft/Bell-412/Models/reflection-frontdoors-px.png</positive-x>
				<negative-x>Aircraft/Bell-412/Models/reflection-front-black.png</negative-x>
				<positive-y>Aircraft/Bell-412/Models/reflection-frontdoors-py.png</positive-y>
				<negative-y>Aircraft/Bell-412/Models/reflection-frontdoors-ny.png</negative-y>
				<positive-z>Aircraft/Bell-412/Models/reflection-front-black.png</positive-z>
				<negative-z>Aircraft/Bell-412/Models/reflection-frontdoors-nz.png</negative-z>	<!-- from top to bottom -->
			</images>
		</texture>
    	<rendering-hint>transparent</rendering-hint>
		<transparent>true</transparent>
		<ambient-correction>
			<use>controls/effects/ambient-correction</use>
		</ambient-correction>
		<texture n="4">
  			<image>Aircraft/Bell-412/Models/mapwhite.png</image>
		</texture>
	</parameters>

</PropertyList>

