<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
<fx>
	<!-- switch high pitch 'tick' -->
	<switch>
		<name>click on</name>
		<mode>once</mode>
		<path>Aircraft/Bell-412/Sounds/click.wav</path>
		<condition>
			<property>/sim/sound/switch</property>
		</condition>
		<volume>
			<offset> 1 </offset>
		</volume>
		<pitch>
			<factor>1.2</factor>
		</pitch>
	</switch>
	<switch>
		<name>click off</name>
		<mode>once</mode>
		<path>Aircraft/Bell-412/Sounds/click.wav</path>
		<condition>
			<not>
				<property>/sim/sound/switch</property>
			</not>
		</condition>
		<volume>
			<offset> 1 </offset>
		</volume>
		<pitch>
			<factor>1.2</factor>
		</pitch>
	</switch>
	
	<!-- power startup and air cond. -->
	<electrical>
		<name>battery on</name>
		<mode>looped</mode>
		<path>Aircraft/Bell-412/Sounds/electricalOn.wav</path>
		<condition>
			<greater-than>
				<property>/sim/sound/electrical</property>
				<value>0</value>
			</greater-than>
		</condition>
		<volume>
			<offset> 2.2 </offset>
		</volume>
		<pitch>
			<property>/sim/sound/electrical</property>
			<factor>1</factor>
			<offset>0.0</offset>
			<max>1</max>
		</pitch>
	</electrical>
	<!--
	<electrical>
		<name>air conditioner</name>
		<mode>looped</mode>
		<path>Aircraft/Bell-412/Sounds/air_cond.wav</path>
		<condition>
			<greater-than>
				<property>/sim/sound/electrical</property>
				<value>0.9</value>
			</greater-than>
		</condition>
		<volume>
			<offset> 0.2 </offset>
		</volume>
	</electrical>
	-->
	<!-- engine -->
	<engine>
		<name>engine-start</name>
		<mode>once</mode>
		<!-- <path>Aircraft/Bell-412/Sounds/v22_turbine_strt_out.wav</path> -->
		<path>Aircraft/Bell-412/Sounds/engine_startup2.wav</path>
		<condition>
			<greater-than>
				<property>sim/model/bell412/state</property>
				<value>0</value>
			</greater-than>
		</condition>
		<volume>
			<factor>1.0</factor>
		</volume>
		<reference-dist>1500</reference-dist>
		<max-dist>5000</max-dist>
	</engine>

	<!--
	<engine>
		<name>turbine</name>
		<mode>looped</mode>
		<path>Sounds/turbine_loop.wav</path>
		<condition>
			<and>
				<greater-than>
					<property>rotors/main/rpm</property>
					<value>50</value>
				</greater-than>
				<greater-than>
					<property>sim/model/bell412/state</property>
					<value>1</value>
				</greater-than>
				<not>
					<property>sim/crashed</property>
				</not>
			</and>
		</condition>
		<volume>
			<property>rotors/gear/torque-sound-filtered</property>
			<factor>0.3</factor>
		</volume>
		<pitch>
			<property>rotors/main/rpm</property>
			<factor>0.003125</factor>
			<offset>-0.0</offset>
			<max>1.03</max>
		</pitch>
		<position>
			<x> -3.7 </x>
			<y> 0 </y>
			<z> 0.4 </z>
		</position>
		<reference-dist>1500</reference-dist>
		<max-dist>5000</max-dist>
	</engine>
	-->

	<!-- outside -->
	<!-- rotor -->
	<rotor>
		<name>rotor</name>
		<mode>looped</mode>
		<path>Aircraft/Bell-412/Sounds/outside_noflap.wav</path>
		<condition>
			<and>
				<greater-than>
					<property>rotors/main/rpm</property>
					<value>50</value>
				</greater-than>
				<not>
					<property>sim/crashed</property>
				</not>
				<not>
					<equals>
						<property>sim/current-view/view-number</property>
						<value>0</value>
					</equals>
				</not>
			</and>
		</condition>
		<volume>
			<property>rotors/gear/torque-sound-filtered</property>
		</volume>
		<pitch>
			<property>rotors/main/rpm</property>
			<factor>0.003125</factor>
			<offset>-0.0</offset>
			<max>2.5</max>
		</pitch>
		<position>
			<x> -4.9 </x>
			<y>  0.0 </y>
			<z>  1.5 </z>
		</position>
		<reference-dist>500</reference-dist>
		<max-dist>5000</max-dist>
	</rotor>

	<rotor>
		<name>tail_rotor</name>
		<mode>looped</mode>
		<path>Aircraft/Bell-412/Sounds/tailrotor.wav</path>
		<condition>
			<and>
				<greater-than>
					<property>rotors/main/rpm</property>
					<value>50</value>
				</greater-than>
				<not>
					<property>sim/crashed</property>
				</not>
				<not>
					<equals>
						<property>sim/current-view/view-number</property>
						<value>0</value>
					</equals>
				</not>
			</and>
		</condition>
		<volume>
			<property>rotors/gear/torque-sound-filtered</property>
			<factor>0.75</factor>
		</volume>
		<pitch>
			<property>rotors/tail/rpm</property>
			<factor>0.0006</factor>
			<offset>-0.0</offset>
			<max>2.5</max>
		</pitch>
		<position>
			<x> -2 </x>
			<y> -0.4 </y>
			<z> 1.3 </z>
		</position>
		<orientation>
			<x> 1 </x>
			<y> . </y>
			<z> 0 </z>
			<inner-angle>110</inner-angle>
			<outer-angle>220</outer-angle>
			<outer-gain>0.1</outer-gain>
		</orientation>
		<reference-dist>500</reference-dist>
		<max-dist>5000</max-dist>
	</rotor>
	
