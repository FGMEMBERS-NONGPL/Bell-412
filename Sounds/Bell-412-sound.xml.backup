				</equals>
			</and>
		</condition>
		<volume>
			<property>rotors/gear/torque-sound-filtered</property>
		</volume>
		<pitch>
			<property>rotors/main/rpm</property>
			<factor>0.00325</factor>
			<offset>-0.0</offset>
			<max>2.5</max>
		</pitch>
		<reference-dist>500</reference-dist>
		<max-dist>1000</max-dist>
	</rotor>
