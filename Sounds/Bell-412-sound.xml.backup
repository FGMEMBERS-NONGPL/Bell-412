	<!--
	<rotor>
		<name>rotorinside</name>
		<mode>looped</mode>
		<path>Aircraft/Bell-412/Sounds/inside_noflap.wav</path>
		<condition>
			<and>
				<greater-than>
					<property>rotors/main/rpm</property>
					<value>50</value>
				</greater-than>
				<not>
					<property>sim/crashed</property>
				</not>
				<equals>
					<property>sim/current-view/view-number</property>
